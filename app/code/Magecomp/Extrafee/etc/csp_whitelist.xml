<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="frame-src">
            <values>
                <value id="square-frame" type="host">*.squarecdn.com</value>
                <value id="web-square-frame" type="host">web.squarecdn.com</value>
                <value id="squareup-frame" type="host">*.squareup.com</value>
                <value id="google-frame" type="host">*.google.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="square-scrip" type="host">*.squarecdn.com</value>
                <value id="web-square-script" type="host">web.squarecdn.com</value>
                <value id="squareup-script" type="host">*.squareup.com</value>
                <value id="google-script" type="host">*.google.com</value>
            </values>
        </policy>
        <policy id="worker-src">
            <values>
                <value id="web-square-worker" type="host">web.squarecdn.com</value>
                <value id="square-worker" type="host">*.squarecdn.com</value>
                <value id="squareup-worker" type="host">*.squareup.com</value>
                <value id="google-worker" type="host">*.google.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
